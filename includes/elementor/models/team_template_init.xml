<?xml version="1.0" encoding="UTF-8"?>
<templates>

    <template>
        <name>List of Teams</name>
        <container>
            <![CDATA[
<!--
==============================
TEMPLATE VARIABLES:
==============================
  'content'
  'pagination'
==============================
-->
<div class="elementor-teamtally-team__template__01">
  <div class="content">
    <div class="header">League Name</div>
    <div class="header">Team Name</div>
    <div class="header">Team NickName</div>
    {{{content}}}
  </div>
  <nav class="pagination">{{{pagination}}}</nav>
</div>
            ]]>
        </container>
        <item>
            <![CDATA[
<!--
==============================
TEMPLATE VARIABLES:
==============================
  'league_id'
  'team_id'
  'team_name'
  'team_nickname'
  'team_history'
  'team_logo_url'
  'league_name'
  'league_country'
  'league_logo_url'
  'class'
==============================
-->
<div class="{{{class}}}"><a href="{{{league_id}}}">{{{league_name}}}</a></div>
<div class="{{{class}}}"><a href="{{{team_id}}}">{{{team_name}}}</a></div>
<div class="{{{class}}}">{{{team_nickname}}}</div>
            ]]>
        </item>
    </template>

</templates>